         <router-link class=\"nav-link\" to=\"/profile\">Profile</router-link>\n            </li>\n            <li class=\"nav-item\">\n              <router-link class=\"nav-link\" to=\"/leaderboard\">Leaderboard</router-link>\n            </li>\n            <li class=\"nav-item\" v-if=\"isLoggedIn\">\n              <router-link class=\"nav-link\" to=\"/hangman\">Hangman</router-link>\n            </li>\n            <li class=\"nav-item\" v-if=\"isLoggedIn\">\n              <button class=\"nav-link btn btn-danger\" @click=\"logout\">Logout</button>\n            </li>\n\n            <!-- Grouping login/signup inside v-else -->\n            <template v-else>\n              <li class=\"nav-item\">\n                <router-link class=\"nav-link\" to=\"/login\">Login</router-link>\n              </li>\n              <li class=\"nav-item\">\n                <router-link class=\"nav-link\" to=\"/signup\">Sign Up</router-link>\n              </li>\n            </template>\n          </ul>\n        </div>\n      </div>\n    </nav>\n\n    <!-- Main content -->\n    <div class=\"container text-center mt-5\">\n      <h1>Welcome to CS Quiz Master! üíª</h1>\n      <p>Sharpen your computer science knowledge and rise to the top of the leaderboard!</p>\n\n      <div class=\"my-4\">\n        <router-link v-if=\"isLoggedIn\" class=\"btn btn-primary btn-lg mx-2\" to=\"/play\">Start Quiz ‚ñ∂Ô∏è</router-link>\n        <template v-else>\n          <router-link class=\"btn btn-success btn-lg mx-2\" to=\"/signup\">Sign Up to Play ‚ú®</router-link>\n          <router-link class=\"btn btn-outline-primary btn-lg mx-2\" to=\"/login\">Login üîë</router-link>\n        </template>\n      </div>\n\n      <h2>How It Works</h2>\n      <div class=\"text-start mx-auto\" style=\"max-width: 600px;\">\n        <p>1. Answer trivia questions related to computer science concepts.</p>\n        <p>2. Earn points for every correct answer.</p>\n        <p>3. Compete with others and climb the leaderboard!</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"HomePage\",\n  computed: {\n  isLoggedIn() {\n    return !!localStorage.getItem(\"user_id\");\n  },\n  isAdmin() {\n    return localStorage.getItem(\"is_admin\") === \"1\";\n  }\n},\n  methods: {\n    logout() {\n      fetch('http://localhost:5001/api/logout', {\n        method: 'POST',\n        headers: { 'Content-Type': 'application/json' },\n      })\n        .then((res) => res.json())\n        .then(() => {\n          localStorage.clear();\n          this.$router.push('/login');\n        })\n        .catch(() => {\n          this.errorMessage = 'An error occurred. Please try again.';\n        });\n    },\n  },\n};\n</script>\n"],"mappings":"AAuEA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,QAAQ,EAAE;IACVC,UAAUA,CAAA,EAAG;MACX,OAAO,CAAC,CAACC,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC;IAC1C,CAAC;IACDC,OAAOA,CAAA,EAAG;MACR,OAAOF,YAAY,CAACC,OAAO,CAAC,UAAU,MAAM,GAAG;IACjD;EACF,CAAC;EACCE,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACPC,KAAK,CAAC,kCAAkC,EAAE;QACxCC,MAAM,EAAE,MAAM;QACdC,OAAO,EAAE;UAAE,cAAc,EAAE;QAAmB;MAChD,CAAC,EACEC,IAAI,CAAEC,GAAG,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC,EACxBF,IAAI,CAAC,MAAM;QACVR,YAAY,CAACW,KAAK,CAAC,CAAC;QACpB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;MAC7B,CAAC,EACAC,KAAK,CAAC,MAAM;QACX,IAAI,CAACC,YAAW,GAAI,sCAAsC;MAC5D,CAAC,CAAC;IACN;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}